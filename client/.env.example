# CodeClash Frontend Environment Variables
# Just copy to .env.local and fill in your values

# API Configuration
REACT_APP_API_URL=http://localhost:3001
REACT_APP_SOCKET_URL=http://localhost:3001

# Google OAuth (for authentication)
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id_here

# Judge0 API (for code execution)
REACT_APP_JUDGE0_URL=https://judge0-ce.p.rapidapi.com
JUDGE0_API_URL=https://judge0-ce.p.rapidapi.com/submissions?base64_encoded=false&wait=true
REACT_APP_JUDGE0_API_KEY=your_judge0_api_key_here

# Analytics (optional)
REACT_APP_GA_TRACKING_ID=your_google_analytics_id_here

# Feature Flags
REACT_APP_ENABLE_TOURNAMENTS=true
REACT_APP_ENABLE_SPECTATOR_MODE=true
REACT_APP_ENABLE_VOICE_CHAT=false

# Environment
REACT_APP_ENVIRONMENT=development
REACT_APP_VERSION=1.0.0

# Debug
REACT_APP_DEBUG_MODE=true
REACT_APP_SHOW_DEV_TOOLS=true
